<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
	SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Routing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"/>   
  </CONNECTORS>

  <PARAMETERS>
	<PARAMETER
      NAME="BEAN"
      FIELD="TEXT"
      NUM_ROW="10"
	  REQUIRED="true"
    >
		<DEFAULT>myBean.class</DEFAULT>
	</PARAMETER>
	<PARAMETER 
		NAME="SPECIFY_METHOD" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="METHOD"
      FIELD="TEXT"
      NUM_ROW="20"
	  REQUIRED="true"
	  SHOW_IF="SPECIFY_METHOD == 'true'"
    >
		<DEFAULT>"Method"</DEFAULT>
	</PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
  </ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.7.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
		   <IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j" MODULE="slf4j-api-1.5.11.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.5.11.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
