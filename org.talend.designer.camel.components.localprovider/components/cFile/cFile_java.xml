<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="true"
	SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Camel</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"/>   
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="INPUT_SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="10" SHOW="false">
		<TABLE READONLY="true">
			<COLUMN NAME="Body"	TYPE="id_String" READONLY="true" />
			<COLUMN NAME="ERROR_CODE" TYPE="id_Integer" READONLY="true" />
		</TABLE>
	</PARAMETER>

	<PARAMETER NAME="CONTEXT_PATH" FIELD="TEXT"  NUM_ROW="1">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="OPTIONCHECK" FIELD="CHECK" REQUIRED="false" NUM_ROW="3" REPOSITORY_VALUE="OPTIONCHECK">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="OPTIONS" FIELD="TABLE" REQUIRED="false" NUM_ROW="61"  SHOW_IF="OPTIONCHECK=='true'"  NB_LINES="3">

      <ITEMS>
       <!-- <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />-->
        <ITEM NAME="OPTIONLIST" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="INITIALDELAY">
            <ITEM NAME="INITIALDELAY"         VALUE="consumer.initialDelay"/>
            <ITEM NAME="FIXEDDELAY"           VALUE="consumer.useFixedDelay"/>
            <ITEM NAME="EXCLUSIVEREADLOCK"    VALUE="consumer.exclusiveReadLock"/>
            <ITEM NAME="RECURSIVE"        VALUE="consumer.recursive"/>
            <ITEM NAME="REGEXPATTERN"     VALUE="consumer.regexPattern"/>
            <ITEM NAME="ALWAYSCONSUME"    VALUE="consumer.alwaysConsume "/>
            <ITEM NAME="TIMESTAMP"        VALUE="consumer.timestamp"/>
            <ITEM NAME="GENERATEEMPTYEXCHANGEWHENIDLE"    VALUE="consumer.generateEmptyExchangeWhenIdle"/>
            <ITEM NAME="EXCLUDEDNAMEPREFIXES"    VALUE="excludedNamePrefix"/>
            <ITEM NAME="PREMOVENAMEPOSTFIX"      VALUE="preMoveNamePostfix"/>
            <ITEM NAME="PREMOVENAMEPREFIX"       VALUE="preMoveNamePrefix"/>
			<ITEM NAME="PREMOVEEXPRESSION"       VALUE="preMoveExpression "/>
			<ITEM NAME="LOCK"                    VALUE="lock"/>
			<ITEM NAME="DELETE"                  VALUE="delete"/>
			<ITEM NAME="NOOP"                    VALUE="noop"/>
			<ITEM NAME="MOVENAMEPREFIX"          VALUE="moveNamePrefix"/>
			<ITEM NAME="MOVENAMEPOSTFIX"         VALUE="moveNamePostfix"/>
			<ITEM NAME="APPEND"                  VALUE="append"/>
			<ITEM NAME="AUTOCREATE"              VALUE="autoCreate"/>
			<ITEM NAME="BUFFERSIZE"              VALUE="bufferSize"/>
			<ITEM NAME="IGNOREFILENAMEHEADER"    VALUE="ignoreFileNameHeader"/>
			<ITEM NAME="EXPRESSION"              VALUE="expression"/>
          </ITEMS>
        </ITEM>
		<ITEM NAME="VALUE" VALUE="&quot;&quot;" FIELD="TEXT" />
      
      </ITEMS>
    </PARAMETER>

   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
	   <IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.5.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
	   </IMPORTS>
  </CODEGENERATION>

   <RETURNS />
</COMPONENT>
