<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Routing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER NAME="REPOSITORY_TYPE" FIELD="CLOSED_LIST" NUM_ROW="5" GROUP="REPOSITORY">
      <ITEMS DEFAULT="MEMORY">
        <ITEM NAME="MEMORY" VALUE="MEMORY"/>
        <ITEM NAME="FILE" VALUE="FILE"/>
      </ITEMS>
    </PARAMETER>
	<PARAMETER
      NAME="CACHE_SIZE"
      FIELD="TEXT"
      NUM_ROW="6"
	  REQUIRED="true"
	  GROUP="REPOSITORY"
	  SHOW_IF="(REPOSITORY_TYPE == 'MEMORY') OR (REPOSITORY_TYPE == 'FILE')"
    >
		<DEFAULT>200</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="FILE_STORE"
      FIELD="FILE"
      NUM_ROW="7"
	  REQUIRED="true"
	  GROUP="REPOSITORY"
	  SHOW_IF="REPOSITORY_TYPE == 'FILE'"
    >
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
  
  
	<PARAMETER 
		NAME="USE_LANGUAGE" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="LANGUAGE"
      FIELD="CLOSED_LIST"
      NUM_ROW="15"
	  REQUIRED="true"
	  SHOW_IF="USE_LANGUAGE == 'true'">
	  <ITEMS DEFAULT="XPATH">
        <ITEM NAME="EL" VALUE="el"/>
        <ITEM NAME="GROOVY" VALUE="groovy"/>
		<ITEM NAME="JXPATH" VALUE="jxpath"/>
		<ITEM NAME="MVEL" VALUE="mvel"/>
		<ITEM NAME="OGNL" VALUE="ognl"/>
		<ITEM NAME="PHP" VALUE="php"/>
		<ITEM NAME="PYTHON" VALUE="python"/>
		<ITEM NAME="RUBY" VALUE="ruby"/>
		<ITEM NAME="SIMPLE" VALUE="simple"/>
		<ITEM NAME="SQL" VALUE="sql"/>
		<ITEM NAME="XQUERY" VALUE="xquery"/>
		<ITEM NAME="HEADER" VALUE="header"/>
		<ITEM NAME="CONSTANT" VALUE="constant"/>
		<ITEM NAME="PROPERTY" VALUE="property"/>
		<ITEM NAME="XPATH" VALUE="xpath"/>
      </ITEMS>
	</PARAMETER>
	<PARAMETER
      NAME="EXPRESSION"
      FIELD="TEXT"
      NUM_ROW="15"
	  REQUIRED="true"
	  SHOW_IF="USE_LANGUAGE == 'false'"
    >
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="PREDICATE"
      FIELD="TEXT"
      NUM_ROW="15"
	  REQUIRED="true"
	  SHOW_IF="USE_LANGUAGE == 'true'"
    >
		<DEFAULT></DEFAULT>
	</PARAMETER>
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.7.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
		   <IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j" MODULE="slf4j-api-1.5.11.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.5.11.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
