<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Miscellaneous</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
		NAME="LOOP_TYPE" 
		FIELD="CLOSED_LIST" 
		NUM_ROW="2">
		<ITEMS DEFAULT="VALUE_TYPE">
			<ITEM NAME="VALUE_TYPE" VALUE="VALUE_TYPE" />
			<ITEM NAME="HEADER_TYPE" VALUE="HEADER_TYPE" />
			<ITEM NAME="EXPRESSION_TYPE" VALUE="EXPRESSION_TYPE" />
		</ITEMS>
	</PARAMETER>
	
	<PARAMETER
      NAME="VALUE"
      FIELD="TEXT"
      NUM_ROW="4"
	  REQUIRED="true"
	  SHOW_IF="LOOP_TYPE == 'VALUE_TYPE'"
    >
	<DEFAULT>1</DEFAULT>
	</PARAMETER>
	
	<PARAMETER
      NAME="HEADER"
      FIELD="TEXT"
      NUM_ROW="4"
	  REQUIRED="true"
	  SHOW_IF="LOOP_TYPE == 'HEADER_TYPE'"
    >
	<DEFAULT>header("loop")</DEFAULT>
	</PARAMETER>
	
	<PARAMETER
      NAME="LANGUAGE"
      FIELD="CLOSED_LIST"
      NUM_ROW="3"
	  REQUIRED="true"
	  SHOW_IF="LOOP_TYPE == 'EXPRESSION_TYPE'">
	  <ITEMS DEFAULT="XPATH">
        <ITEM NAME="EL" VALUE="el"/>
        <ITEM NAME="GROOVY" VALUE="groovy"/>
		<ITEM NAME="JXPATH" VALUE="jxpath"/>
		<ITEM NAME="MVEL" VALUE="mvel"/>
		<ITEM NAME="OGNL" VALUE="ognl"/>
		<ITEM NAME="PHP" VALUE="php"/>
		<ITEM NAME="PYTHON" VALUE="python"/>
		<ITEM NAME="RUBY" VALUE="ruby"/>
		<ITEM NAME="SIMPLE" VALUE="simple"/>
		<ITEM NAME="SQL" VALUE="sql"/>
		<ITEM NAME="XQUERY" VALUE="xquery"/>
		<ITEM NAME="HEADER" VALUE="header"/>
		<ITEM NAME="CONSTANT" VALUE="constant"/>
		<ITEM NAME="PROPERTY" VALUE="property"/>
		<ITEM NAME="XPATH" VALUE="xpath"/>
      </ITEMS>
	</PARAMETER>
	
	<PARAMETER
      NAME="EXPRESSION"
      FIELD="TEXT"
      NUM_ROW="4"
	  REQUIRED="true"
	  SHOW_IF="LOOP_TYPE == 'EXPRESSION_TYPE'"
    >
	<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.7.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
		   <IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j" MODULE="slf4j-api-1.5.11.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.5.11.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
