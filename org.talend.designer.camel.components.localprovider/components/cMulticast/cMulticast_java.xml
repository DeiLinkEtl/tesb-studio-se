<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Routing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
	  <CONNECTOR CTYPE="ROUTE" MAX_OUTPUT="1" NAME="BLOCK"/>    
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
		NAME="URIS"
		FIELD="TABLE"
		NUM_ROW="1"
		NB_LINES="5"
		REQUIRED="true"
    >
      <ITEMS>
        <ITEM NAME="URI" VALUE="&quot;&quot;" FIELD="TEXT" />
	  </ITEMS>
	</PARAMETER>
	  
	<PARAMETER 
		NAME="USE_AGGREGATION_STRATEGY" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="5">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER NAME="AGGREGATION_STRATEGY" FIELD="TEXT" REQUIRED="true" NUM_ROW="5" SHOW_IF="USE_AGGREGATION_STRATEGY == 'true'">
		<DEFAULT>AggregationStrategy</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER 
		NAME="PARALLEL_PROCESSING" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="2">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER
		NAME="USE_TIMEOUT"
		FIELD="CHECK"
		NUM_ROW="3"
		REQUIRED="true"
		SHOW_IF="(PARALLEL_PROCESSING == 'true')">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	 	  
	<PARAMETER
		NAME="TIMEOUT"
		FIELD="TEXT"
		NUM_ROW="4"
		REQUIRED="true"
		SHOW_IF="((PARALLEL_PROCESSING == 'true') and (USE_TIMEOUT == 'true'))">
	<DEFAULT>1000</DEFAULT>
	</PARAMETER>
	
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.7.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
		   <IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j" MODULE="slf4j-api-1.5.11.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.5.11.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
