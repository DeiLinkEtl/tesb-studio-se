<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Routing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0"/>
	  <CONNECTOR CTYPE="ROUTE" MAX_OUTPUT="1" NAME="BLOCK"/>    
  </CONNECTORS>

   <PARAMETERS>

		<PARAMETER
      NAME="URIS"
      FIELD="TEXT"
      NUM_ROW="1"
	  REQUIRED="true"
    >
		<DEFAULT>""</DEFAULT>
	  </PARAMETER>
	  
	  <PARAMETER 
		NAME="AGGREGATION_STRATEGY" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
	  </PARAMETER>
	  
	 <PARAMETER NAME="CODE" FIELD="MEMO_JAVA" REQUIRED="false" NUM_ROW="6" NB_LINES="3" CONTEXT="begin"
      	SHOW_IF="(AGGREGATION_STRATEGY == 'true')"
      	>
	      <DEFAULT>return oldExchange;</DEFAULT>
	  </PARAMETER>
	  
	   <PARAMETER 
		NAME="PARALLEL_PROCESSING" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
	  </PARAMETER>
	  
	  <PARAMETER
      NAME="USE_TIMEOUT"
      FIELD="CHECK"
      NUM_ROW="3"
	 	  REQUIRED="true"
	 	  SHOW_IF="(PARALLEL_PROCESSING == 'true')"
	 	  >
	 	  <DEFAULT>false</DEFAULT>
	  </PARAMETER>
	 	  
		<PARAMETER
      NAME="TIMEOUT"
      FIELD="TEXT"
      NUM_ROW="4"
	 	  REQUIRED="true"
	 	  SHOW_IF="((PARALLEL_PROCESSING == 'true') and (USE_TIMEOUT == 'true'))"
	 	  >
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>
	
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

   <CODEGENERATION/>

   <RETURNS />
</COMPONENT>
