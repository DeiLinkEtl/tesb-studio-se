<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
	SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Transformation</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"/>  
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="HEADER" FIELD="TEXT"  NUM_ROW="1" REQUIRED="true">
        <DEFAULT>""</DEFAULT>
    </PARAMETER>
	<PARAMETER 
		NAME="USE_BEAN" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
    <PARAMETER NAME="LANGUAGES" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="3" SHOW_IF="USE_BEAN == 'false'">
		<ITEMS DEFAULT="XPATH">
			<ITEM NAME="XPATH"      VALUE="xpath" />
			<ITEM NAME="EL"         VALUE="el" />
			<ITEM NAME="GROOVY"     VALUE="groovy" />
			<ITEM NAME="JAVASCRIPT" VALUE="javascript" />
			<ITEM NAME="JXPATH"     VALUE="jxpath" />
			<ITEM NAME="MVEL"       VALUE="mvel" />
			<ITEM NAME="OGNL"       VALUE="ognl" />
			<ITEM NAME="XQUERY"     VALUE="xquery" />
			<ITEM NAME="PHP"        VALUE="php" />
			<ITEM NAME="PYTHON"     VALUE="python" />
			<ITEM NAME="RUBY"       VALUE="ruby" />
			<ITEM NAME="SIMPLE"     VALUE="simple" />
			<ITEM NAME="SQL"        VALUE="sql" />
		</ITEMS>
	</PARAMETER>
    
    <PARAMETER NAME="EXPRESSION" FIELD="TEXT"  NUM_ROW="5" REQUIRED="true" SHOW_IF="USE_BEAN == 'false'">
        <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="BEAN" FIELD="TEXT"  NUM_ROW="10" REQUIRED="true" SHOW_IF="USE_BEAN == 'true'">
        <DEFAULT>myBean.class</DEFAULT>
    </PARAMETER>
    
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		   <IMPORT NAME="apache-camel" MODULE="camel-core-2.7.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar" REQUIRED="true" />
		   <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
		   <IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j" MODULE="slf4j-api-1.5.11.jar" REQUIRED="true" />
		   <IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.5.11.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
