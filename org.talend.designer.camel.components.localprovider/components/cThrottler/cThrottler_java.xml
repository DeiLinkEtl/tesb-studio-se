<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processor</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0"/>
	<CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
      NAME="MESSAGE_COUNT"
      FIELD="TEXT"
      NUM_ROW="10"
	  REQUIRED="true"
    >
		<DEFAULT>100</DEFAULT>
	</PARAMETER>
	<PARAMETER 
		NAME="SET_TIME_PERIOD" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="TIME_PERIOD"
      FIELD="TEXT"
      NUM_ROW="15"
	  REQUIRED="true"
	  SHOW_IF="SET_TIME_PERIOD == 'true'"
    >
		<DEFAULT>1000</DEFAULT>
	</PARAMETER>
	<PARAMETER 
		NAME="USE_ASYNC_DELAYING" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

   <CODEGENERATION/>

   <RETURNS />
